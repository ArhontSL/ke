{
  "name": "@bestdoctor/ke",
  "version": "0.0.4",
  "description": "BestDoctor back-office UI constructor",
  "author": "pro100filipp",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "axios": "^0.19.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@bestdoctor/eslint-config": "^0.5.4",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "prettier": "^1.19.1",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint --cache --ext .ts,.tsx src",
    "prepare": "yarn build",
    "preversion": "yarn lint ",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "eslintConfig": {
    "extends": "@bestdoctor"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/best-doctor/ke.git"
  },
  "bugs": {
    "url": "https://github.com/best-doctor/ke/issues"
  },
  "homepage": "https://github.com/best-doctor/ke#readme",
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
